    "eslint-plugin-react": "^7.11.1",
    "moxios": "^0.4.0",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/setupTests.js",
      "!src/index.js",
      "!src/store.js"
    ]
  }
