  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npm run start:dev --prefix api\"",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test -u --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "build:prod": "npm run build --prefix api && npm run build",
    "start:prod": "concurrently \"npm run start --prefix api\" \"serve -s build\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
